{
	"name": "helloworld-sample",
	"displayName": "helloworld-sample",
	"description": "HelloWorld example for VS Code",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-sample",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"browser": "./dist/browser/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:hello-ext ./tsconfig.json",
		"watch": "gulp watch-extension:hello-ext",
		"compile": "gulp compile-extension:hello-ext"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.32.0",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.3.0"
	}
}
